  <process id="leaveApplicationProcess2" name="请假申请流程" isExecutable="true">
    <startEvent id="start" name="开始申请" flowable:formFieldValidation="true">
      <extensionElements>
        <flowable:formProperty id="leaveType" name="请假类型" type="enum" required="true">
          <flowable:value id="annual" name="年假"/>
          <flowable:value id="sick" name="病假"/>
          <flowable:value id="personal" name="事假"/>
          <flowable:value id="marriage" name="婚假"/>
          <flowable:value id="maternity" name="产假"/>
          <flowable:value id="funeral" name="丧假"/>
        </flowable:formProperty>
        <flowable:formProperty id="startDate" name="开始日期" type="date" datePattern="yyyy-MM-dd" required="true"/>
        <flowable:formProperty id="endDate" name="结束日期" type="date" datePattern="yyyy-MM-dd" required="true"/>
        <flowable:formProperty id="reason" name="请假原因" type="string" required="true"/>
        <flowable:formProperty id="attachment" name="附件" type="string"/>
      </extensionElements>
    </startEvent>
    <serviceTask id="calculateLeaveDays" name="计算请假天数" flowable:class="vip.fairy.service.CalculateLeaveDaysService">
      <extensionElements>
        <flowable:field name="startDate">
          <flowable:expression><![CDATA[ ${startDate} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="endDate">
          <flowable:expression><![CDATA[ ${endDate} ]]></flowable:expression>
        </flowable:field>
      </extensionElements>
    </serviceTask>
    <exclusiveGateway id="checkLeaveTypeAndDays" name="检查请假类型和天数"/>
    <userTask id="applyTask" name="提交请假申请" flowable:candidateGroups="employees" flowable:formFieldValidation="true">
      <extensionElements>
        <flowable:formProperty id="leaveType" name="请假类型" type="enum" readable="false" writable="false"/>
        <flowable:formProperty id="startDate" name="开始日期" type="date" datePattern="yyyy-MM-dd" readable="false" writable="false"/>
        <flowable:formProperty id="endDate" name="结束日期" type="date" datePattern="yyyy-MM-dd" readable="false" writable="false"/>
        <flowable:formProperty id="leaveDays" name="请假天数" type="integer" readable="false" writable="false"/>
        <flowable:formProperty id="reason" name="请假原因" type="string" readable="false" writable="false"/>
        <flowable:formProperty id="attachment" name="附件" type="string" readable="false" writable="false"/>
        <flowable:formProperty id="confirm" name="确认信息无误" type="boolean" required="true"/>
      </extensionElements>
    </userTask>
    <userTask id="supervisorApproval" name="直接上级审批" flowable:assignee="${supervisor}" flowable:formFieldValidation="true">
      <extensionElements>
        <flowable:formProperty id="supervisorComment" name="审批意见" type="string"/>
        <flowable:formProperty id="supervisorApprovalResult" name="审批结果" type="enum" required="true">
          <flowable:value id="approved" name="同意"/>
          <flowable:value id="rejected" name="拒绝"/>
          <flowable:value id="revised" name="需修改"/>
        </flowable:formProperty>
        <modeler:initiator-can-complete xmlns:modeler="http://flowable.org/modeler"><![CDATA[ false ]]></modeler:initiator-can-complete>
      </extensionElements>
    </userTask>
    <serviceTask id="reminderTask" name="发送超时提醒" flowable:class="vip.fairy.service.ReminderService">
      <extensionElements>
        <flowable:field name="taskId">
          <flowable:expression><![CDATA[ ${execution.getProcessInstanceId()} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="taskName">
          <flowable:expression><![CDATA[ ${supervisorApproval.name} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="assignee">
          <flowable:expression><![CDATA[ ${supervisor} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="employeeName">
          <flowable:expression><![CDATA[ ${employeeName} ]]></flowable:expression>
        </flowable:field>
      </extensionElements>
    </serviceTask>
    <userTask id="managerApproval" name="部门经理审批" flowable:assignee="${departmentManager}" flowable:formFieldValidation="true">
      <extensionElements>
        <flowable:formProperty id="managerComment" name="审批意见" type="string"/>
        <flowable:formProperty id="managerApprovalResult" name="审批结果" type="enum" required="true">
          <flowable:value id="approved" name="同意"/>
          <flowable:value id="rejected" name="拒绝"/>
          <flowable:value id="revised" name="需修改"/>
        </flowable:formProperty>
        <modeler:initiator-can-complete xmlns:modeler="http://flowable.org/modeler"><![CDATA[ false ]]></modeler:initiator-can-complete>
      </extensionElements>
    </userTask>
    <userTask id="hrApproval" name="HR审批" flowable:candidateGroups="hr" flowable:formFieldValidation="true">
      <extensionElements>
        <flowable:formProperty id="hrComment" name="审批意见" type="string"/>
        <flowable:formProperty id="hrApprovalResult" name="审批结果" type="enum" required="true">
          <flowable:value id="approved" name="同意"/>
          <flowable:value id="rejected" name="拒绝"/>
          <flowable:value id="revised" name="需修改"/>
        </flowable:formProperty>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="checkSupervisorResult" name="检查上级审批结果"/>
    <exclusiveGateway id="checkManagerResult" name="检查经理审批结果"/>
    <exclusiveGateway id="checkHrResult" name="检查HR审批结果"/>
    <endEvent id="leaveRejected" name="请假申请被拒绝"/>
    <userTask id="reviseApplication" name="修改请假申请" flowable:assignee="${employeeId}" flowable:formFieldValidation="true">
      <extensionElements>
        <flowable:formProperty id="leaveType" name="请假类型" type="enum" required="true">
          <flowable:value id="annual" name="年假"/>
          <flowable:value id="sick" name="病假"/>
          <flowable:value id="personal" name="事假"/>
          <flowable:value id="marriage" name="婚假"/>
          <flowable:value id="maternity" name="产假"/>
          <flowable:value id="funeral" name="丧假"/>
        </flowable:formProperty>
        <flowable:formProperty id="startDate" name="开始日期" type="date" datePattern="yyyy-MM-dd" required="true"/>
        <flowable:formProperty id="endDate" name="结束日期" type="date" datePattern="yyyy-MM-dd" required="true"/>
        <flowable:formProperty id="reason" name="请假原因" type="string" required="true"/>
        <flowable:formProperty id="attachment" name="附件" type="string"/>
        <flowable:formProperty id="rejectionReason" name="驳回原因" type="string" readable="false" writable="false"/>
        <modeler:initiator-can-complete xmlns:modeler="http://flowable.org/modeler"><![CDATA[ false ]]></modeler:initiator-can-complete>
      </extensionElements>
    </userTask>
    <serviceTask id="leaveApproved" name="请假申请已批准" flowable:class="com.leave.service.LeaveApprovedService">
      <extensionElements>
        <flowable:field name="employeeId">
          <flowable:expression><![CDATA[ ${employeeId} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="employeeName">
          <flowable:expression><![CDATA[ ${employeeName} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="leaveType">
          <flowable:expression><![CDATA[ ${leaveType} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="startDate">
          <flowable:expression><![CDATA[ ${startDate} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="endDate">
          <flowable:expression><![CDATA[ ${endDate} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="leaveDays">
          <flowable:expression><![CDATA[ ${leaveDays} ]]></flowable:expression>
        </flowable:field>
      </extensionElements>
    </serviceTask>
    <endEvent id="end" name="流程结束"/>
    <serviceTask id="sendNotification" name="发送通知" flowable:class="com.leave.service.NotificationService">
      <extensionElements>
        <flowable:field name="employeeId">
          <flowable:expression><![CDATA[ ${employeeId} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="employeeName">
          <flowable:expression><![CDATA[ ${employeeName} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="leaveType">
          <flowable:expression><![CDATA[ ${leaveType} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="startDate">
          <flowable:expression><![CDATA[ ${startDate} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="endDate">
          <flowable:expression><![CDATA[ ${endDate} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="leaveDays">
          <flowable:expression><![CDATA[ ${leaveDays} ]]></flowable:expression>
        </flowable:field>
        <flowable:field name="status">
          <flowable:expression><![CDATA[ ${status} ]]></flowable:expression>
        </flowable:field>
      </extensionElements>
    </serviceTask>
    <sequenceFlow id="startToCalculate" sourceRef="start" targetRef="calculateLeaveDays"/>
    <sequenceFlow id="calculateToCheck" sourceRef="calculateLeaveDays" targetRef="checkLeaveTypeAndDays"/>
    <sequenceFlow id="applyToSupervisor" sourceRef="applyTask" targetRef="supervisorApproval"/>
    <sequenceFlow id="checkToManagerApproval" sourceRef="checkSupervisorResult" targetRef="managerApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${supervisorApprovalResult == 'approved' && leaveType == 'sick' && leaveDays > 1 || supervisorApprovalResult == 'approved' && leaveType != 'sick' && leaveDays > 3} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="approvedToNotification" sourceRef="leaveApproved" targetRef="sendNotification"/>
    <sequenceFlow id="reviseToCalculate" sourceRef="reviseApplication" targetRef="calculateLeaveDays"/>
    <sequenceFlow id="hrToCheck" sourceRef="hrApproval" targetRef="checkHrResult"/>
    <sequenceFlow id="checkToApply" sourceRef="checkLeaveTypeAndDays" targetRef="applyTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${leaveType != 'sick' && leaveDays <= 3} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="checkToManager" sourceRef="checkLeaveTypeAndDays" targetRef="managerApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${leaveType == 'sick' && leaveDays > 1 || leaveType != 'sick' && leaveDays > 3} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="notificationToEnd" sourceRef="sendNotification" targetRef="end"/>
    <sequenceFlow id="checkToRejected" sourceRef="checkSupervisorResult" targetRef="leaveRejected">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${supervisorApprovalResult == 'rejected'} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="rejectedToNotification" sourceRef="leaveRejected" targetRef="sendNotification"/>
    <sequenceFlow id="managerToCheck" sourceRef="managerApproval" targetRef="checkManagerResult"/>
    <sequenceFlow id="managerCheckToRejected" sourceRef="checkManagerResult" targetRef="leaveRejected">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${managerApprovalResult == 'rejected'} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="managerCheckToHr" sourceRef="checkManagerResult" targetRef="hrApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${managerApprovalResult == 'approved'} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="managerCheckToRevise" sourceRef="checkManagerResult" targetRef="reviseApplication">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${managerApprovalResult == 'revised'} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="hrCheckToRejected" sourceRef="checkHrResult" targetRef="leaveRejected">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${hrApprovalResult == 'rejected'} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="hrCheckToApproved" sourceRef="checkHrResult" targetRef="leaveApproved">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${hrApprovalResult == 'approved'} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="hrCheckToRevise" sourceRef="checkHrResult" targetRef="reviseApplication">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${hrApprovalResult == 'revised'} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="checkToHrApproval" sourceRef="checkSupervisorResult" targetRef="hrApproval">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${supervisorApprovalResult == 'approved' && leaveType != 'sick' && leaveDays <= 3} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="checkToRevise" sourceRef="checkSupervisorResult" targetRef="reviseApplication">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[ ${supervisorApprovalResult == 'revised'} ]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="supervisorToCheck" sourceRef="supervisorApproval" targetRef="checkSupervisorResult"/>
    <sequenceFlow id="sid-1f4662ac-aa70-4bed-ace1-4a5a1c80b152" sourceRef="supervisorTimer" targetRef="reminderTask"/>
    <boundaryEvent id="supervisorTimer" name="审批超时" attachedToRef="supervisorApproval" cancelActivity="false">
      <timerEventDefinition>
        <timeDuration>PT2M</timeDuration>
      </timerEventDefinition>
    </boundaryEvent>
  </process>
